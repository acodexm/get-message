{
  "name": "get-message",
  "version": "1.0.10",
  "description": "Small lib as a replacement for react-intl",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore '**/*.test.js'",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "prettier": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acodexm/get-message.git"
  },
  "keywords": [
    "react-intl",
    "messages",
    "function",
    "javascript",
    "reactjs",
    "npm",
    "node",
    "util"
  ],
  "author": "Acodexm <adamkx7x@gmail.com> (https://github.com/acodexm)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acodexm/get-message/issues"
  },
  "homepage": "https://github.com/acodexm/get-message#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prefer-arrow": "^1.1.3",
    "eslint-plugin-prettier": "^3.0.0",
    "expect": "^1.9.0",
    "husky": "^1.1.3",
    "intl-format-cache": "^2.1.0",
    "intl-messageformat": "^2.2.0",
    "intl-relativeformat": "^2.1.0",
    "invariant": "^2.2.4",
    "jest": "^23.6.0",
    "jest-coverage-badges": "^1.0.0",
    "prettier": "^1.15.1",
    "pretty-quick": "^1.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  }
}
